<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homeContainerVM" type="com.jamshaidsapplication.app.modules.homecontainer.data.viewmodel.HomeContainerVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnbg"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyledeep_purple_500">
        <FrameLayout android:id="@+id/fragmentContainer"
            android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent"/>
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameBottombarHome"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_1pxh" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStack"
                android:layout_height="@dimen/_112pxv" android:layout_width="@dimen/_375pxh">
                <FrameLayout android:id="@+id/frameStacksubtract"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_80pxv"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_375pxh">
                    <ImageView android:id="@+id/imageSubtract"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_80pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_subtract"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_subtract"/>
                    <LinearLayout android:id="@+id/linearRow"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_28pxv"
                        android:layout_marginEnd="@dimen/_32pxh"
                        android:layout_marginStart="@dimen/_35pxh"
                        android:layout_marginTop="@dimen/_28pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/linearRowvectorFour"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <FrameLayout android:id="@+id/frameActive"
                                android:layout_height="@dimen/_24pxv" android:layout_width="@dimen/_24pxh">
                                <ImageView
                                    android:id="@+id/imageVectorFour"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_19pxh"
                                    android:layout_marginHorizontal="@dimen/_2pxh"
                                    android:layout_marginVertical="@dimen/_2pxv"
                                    android:layout_width="@dimen/_20pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_vector_deep_purple_500"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_deep_purple_500"/>
                                <ImageView
                                    android:id="@+id/imageHomeTwo"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_19pxh"
                                    android:layout_marginHorizontal="@dimen/_2pxh"
                                    android:layout_marginVertical="@dimen/_2pxv"
                                    android:layout_width="@dimen/_20pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_home2"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_home2"/>
                            </FrameLayout>
                            <ImageView android:id="@+id/imageShop"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginStart="@dimen/_39pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="@dimen/_19pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_shop"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_shop"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearRowgroupone"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_144pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.5"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageGroupOne"
                                android:layout_height="@dimen/_19pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_group1"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_group1"/>

                            <ImageView
                                android:id="@+id/imageUsersquare"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginStart="@dimen/_40pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_usersquare"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_usersquare" />
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/floatingBtnFloatingactionbutton"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_155pxh"
                    android:layout_marginStart="@dimen/_155pxh"
                    android:layout_width="wrap_content"
                    app:fabCustomSize="@dimen/_64pxh"
                    app:srcCompat="@drawable/img_group1_white_a700" app:tint="@color/white_A700"/>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
