            android:text="@{bodyFontRowModel.txtH130One}"
            style="@style/txtCircularStdmedium30" tools:text="@string/lbl_h1_30"/>
        <TextView android:ellipsize="end" android:id="@+id/txtH130Two"
            android:layout_height="wrap_content"
