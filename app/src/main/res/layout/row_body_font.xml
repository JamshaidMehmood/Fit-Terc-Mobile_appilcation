<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="bodyFontRowModel" type="com.jamshaidsapplication.app.modules.bodyfont.data.model.BodyFontRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowh130"
