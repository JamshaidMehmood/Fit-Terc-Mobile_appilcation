<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="numbericFontVM" type="com.jamshaidsapplication.app.modules.numbericfont.data.viewmodel.NumbericFontVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearNumbericFont"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <TextView android:ellipsize="end"
            android:id="@+id/txtTypography"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_64pxh"
            android:layout_marginStart="@dimen/_64pxh"
            android:layout_marginTop="@dimen/_42pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{numbericFontVM.numbericFontModel.txtTypography}"
            style="@style/txtCircularStdmedium24" tools:text="@string/lbl_typography"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtArcivoBlack"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_64pxh"
            android:layout_marginStart="@dimen/_64pxh"
            android:layout_marginTop="@dimen/_42pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_2pxh"
            android:singleLine="true"
            android:text="@{numbericFontVM.numbericFontModel.txtArcivoBlack}"
            style="@style/txtBebasNeue18" tools:text="@string/lbl_bebas_neue"/>
