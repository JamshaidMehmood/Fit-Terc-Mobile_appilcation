            android:id="@+id/txtHeading616"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_64pxh"
            android:layout_marginStart="@dimen/_64pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_2pxh"
            android:singleLine="true"
            android:text="@{numbericFontVM.numbericFontModel.txtHeading616}"
            style="@style/txtBebasNeue16_1" tools:text="@string/lbl_heading_6_16"/>
